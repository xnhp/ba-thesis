@online{rocheteau_predicting_2021,
  title = {Predicting {{Patient Outcomes}} with {{Graph Representation Learning}}},
  author = {Rocheteau, Emma and Tong, Catherine and Veličković, Petar and Lane, Nicholas and Liò, Pietro},
