@incollection{abu-khzam_ClusterEditingVertex_2018,
  title = {Cluster {{Editing}} with {{Vertex Splitting}}},
  booktitle = {Combinatorial {{Optimization}}},
  author = {Abu-Khzam, Faisal N. and Egan, Judith and Gaspers, Serge and Shaw, Alexis and Shaw, Peter},
