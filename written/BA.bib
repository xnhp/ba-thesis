@article{henry_ConvertingDiseaseMaps_2021,
  title = {Converting Disease Maps into Heavyweight Ontologies: General Methodology and Application to {{Alzheimer}}'s Disease},
  shorttitle = {Converting Disease Maps into Heavyweight Ontologies},
  author = {Henry, Vincent and Moszer, Ivan and Dameron, Olivier and Vila Xicota, Laura and Dubois, Bruno and Potier, Marie-Claude and Hofmann-Apitius, Martin and Colliot, Olivier and {INSIGHT-preAD Study Group}},
  date = {2021-02-16},
