  keywords = {Acinetobacter baumannii,Animals,Anti-Bacterial Agents,antibiotic resistance,antibiotic tolerance,antibiotics,Cheminformatics,Clostridioides difficile,Databases; Chemical,drug discovery,Drug Discovery,machine learning,Machine Learning,Mice,Mice; Inbred BALB C,Mice; Inbred C57BL,Mycobacterium tuberculosis,Small Molecule Libraries},
