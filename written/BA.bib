@article{furuta_MetabolicGenesCancer_2010,
  title = {Metabolic Genes in Cancer: Their Roles in Tumor Progression and Clinical Implications},
  shorttitle = {Metabolic Genes in Cancer},
  author = {Furuta, Eiji and Okuda, Hiroshi and Kobayashi, Aya and Watabe, Kounosuke},
  date = {2010-04},
  journaltitle = {Biochimica Et Biophysica Acta},
