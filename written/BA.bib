@inproceedings{cong_importance_2021,
  title = {On the {{Importance}} of {{Sampling}} in {{Learning Graph Convolutional Networks}}},
  author = {Cong, Weilin and Ramezani, M. and Mahdavi, M.},
  date = {2021},
  abstract = {Graph Convolutional Networks (GCNs) have achieved impressive empirical advancement across a wide variety of graph-related applications. Despite their great success, training GCNs on large graphs suffers from computational and memory issues. A potential path to circumvent these obstacles is sampling-based methods, where at each layer a subset of nodes is sampled. Although recent studies have empirically demonstrated the effectiveness of sampling-based methods, these works lack theoretical convergence guarantees under realistic settings and cannot fully leverage the information of evolving parameters during optimization. In this paper, we describe and analyze a general \textbackslash textbf\{\textbackslash textit\{doubly variance reduction\}\} schema that can accelerate any sampling method under the memory budget. The motivating impetus for the proposed schema is a careful analysis for the variance of sampling methods where it is shown that the induced variance can be decomposed into node embedding approximation variance (\textbackslash emph\{zeroth-order variance\}) during forward propagation and layerwise-gradient variance (\textbackslash emph\{first-order variance\}) during backward propagation. We theoretically analyze the convergence of the proposed schema and show that it enjoys an \$\textbackslash mathcal\{O\}(1/T)\$ convergence rate. We complement our theoretical results by integrating the proposed schema in different sampling methods and applying them to different large real-world graphs. Code is public available at\textasciitilde\textbackslash url\{https://github.com/CongWeilin/SGCN.git\}.},
  file = {C\:\\Users\\Ben\\Dropbox\\Uni\\Uni Vault\\zotero-mdnotes-export\\cong_importance_2021.md;C\:\\Users\\Ben\\Zotero\\storage\\HU8ZZYJL\\Cong et al_2021_On the Importance of Sampling in Learning Graph Convolutional Networks.pdf}
}

@online{corso_PrincipalNeighbourhoodAggregation_2020,
  title = {Principal {{Neighbourhood Aggregation}} for {{Graph Nets}}},
  author = {Corso, Gabriele and Cavalleri, Luca and Beaini, Dominique and Liò, Pietro and Veličković, Petar},
  date = {2020-12-31},
