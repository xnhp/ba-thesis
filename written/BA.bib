  author = {Wang, Hao and Marcišauskas, Simonas and Sánchez, Benjamín J. and Domenzain, Iván and Hermansson, Daniel and Agren, Rasmus and Nielsen, Jens and Kerkhoven, Eduard J.},
  editor = {Ouzounis, Christos A.},
  date = {2018-10-18},
