@article{aoki_ConvolutionalNeuralNetworks_2018,
  title = {Convolutional Neural Networks for Classification of Alignments of Non-Coding {{RNA}} Sequences},
  author = {Aoki, Genta and Sakakibara, Yasubumi},
  date = {2018-07-01},
  journaltitle = {Bioinformatics},
  shortjournal = {Bioinformatics},
  volume = {34},
  number = {13},
  pages = {i237-i244},
  issn = {1367-4803},
  doi = {10.1093/bioinformatics/bty228},
  url = {https://doi.org/10.1093/bioinformatics/bty228},
  urldate = {2021-10-06},
  abstract = {The convolutional neural network (CNN) has been applied to the classification problem of DNA sequences, with the additional purpose of motif discovery. The training of CNNs with distributed representations of four nucleotides has successfully derived position weight matrices on the learned kernels that corresponded to sequence motifs such as protein-binding sites.We propose a novel application of CNNs to classification of pairwise alignments of sequences for accurate clustering of sequences and show the benefits of the CNN method of inputting pairwise alignments for clustering of non-coding RNA (ncRNA) sequences and for motif discovery. Classification of a pairwise alignment of two sequences into positive and negative classes corresponds to the clustering of the input sequences. After we combined the distributed representation of RNA nucleotides with the secondary-structure information specific to ncRNAs and furthermore with mapping profiles of next-generation sequence reads, the training of CNNs for classification of alignments of RNA sequences yielded accurate clustering in terms of ncRNA families and outperformed the existing clustering methods for ncRNA sequences. Several interesting sequence motifs and secondary-structure motifs known for the snoRNA family and specific to microRNA and tRNA families were identified.The source code of our CNN software in the deep-learning framework Chainer is available at http://www.dna.bio.keio.ac.jp/cnn/, and the dataset used for performance evaluation in this work is available at the same URL.},
  file = {C\:\\Users\\Ben\\Obsidian Vaults\\Uni Vault\\zotero-mdnotes-export\\aoki_ConvolutionalNeuralNetworks_2018.md;C\:\\Users\\Ben\\Zotero\\storage\\2YIPTXZ3\\Aoki_Sakakibara_2018_Convolutional neural networks for classification of alignments of non-coding.pdf;C\:\\Users\\Ben\\Zotero\\storage\\ZANTSMI2\\5045743.html}
}

@online{arya_HyperSAGEGeneralizingInductive_2020,
  title = {{{HyperSAGE}}: Generalizing {{Inductive Representation Learning}} on {{Hypergraphs}}},
  shorttitle = {{{HyperSAGE}}},
  author = {Arya, Devanshu and Gupta, Deepak K. and Rudinac, Stevan and Worring, Marcel},
  date = {2020-10-09},
  eprint = {2010.04558},
  eprinttype = {arxiv},
  primaryclass = {cs, stat},
  url = {http://arxiv.org/abs/2010.04558},
  urldate = {2021-08-31},
  abstract = {Graphs are the most ubiquitous form of structured data representation used in machine learning. They model, however, only pairwise relations between nodes and are not designed for encoding the higher-order relations found in many real-world datasets. To model such complex relations, hypergraphs have proven to be a natural representation. Learning the node representations in a hypergraph is more complex than in a graph as it involves information propagation at two levels: within every hyperedge and across the hyperedges. Most current approaches first transform a hypergraph structure to a graph for use in existing geometric deep learning algorithms. This transformation leads to information loss, and sub-optimal exploitation of the hypergraph’s expressive power. We present HyperSAGE, a novel hypergraph learning framework that uses a two-level neural message passing strategy to accurately and efficiently propagate information through hypergraphs. The flexible design of HyperSAGE facilitates different ways of aggregating neighborhood information. Unlike the majority of related work which is transductive, our approach, inspired by the popular GraphSAGE method, is inductive. Thus, it can also be used on previously unseen nodes, facilitating deployment in problems such as evolving or partially observed hypergraphs. Through extensive experimentation, we show that HyperSAGE outperforms state-of-the-art hypergraph learning methods on representative benchmark datasets. We also demonstrate that the higher expressive power of HyperSAGE makes it more stable in learning node representations as compared to the alternatives.},
  archiveprefix = {arXiv},
  langid = {english},
  keywords = {Computer Science - Machine Learning,Statistics - Machine Learning},
  file = {C\:\\Users\\Ben\\Zotero\\storage\\VU3AH3JK\\Arya et al. - 2020 - HyperSAGE Generalizing Inductive Representation L.pdf}
}

